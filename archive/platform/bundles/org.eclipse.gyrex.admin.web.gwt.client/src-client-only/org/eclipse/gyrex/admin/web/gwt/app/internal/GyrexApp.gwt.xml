<module>

  <!-- Inherit the core Web Toolkit stuff.  -->
  <inherits name='com.google.gwt.user.User' />
	
  <!-- Inherit the CWT stuff. -->
  <inherits name='org.eclipse.gyrex.gwt.common.Common' />
  <inherits name='org.eclipse.gyrex.toolkit.gwt.Ui' />

  <!-- Inherit the APP services. -->
  <inherits name='org.eclipse.gyrex.admin.web.gwt.app.internal.services.GyrexAppServices' />
	
  <!-- don't publish template files -->
  <public path="public">
    <exclude name="**/*.template" />
  </public>
	
  <!-- inject common CWT/GWT specific styling -->
  <stylesheet src="css/cwt-style.css"/>
	
</module>