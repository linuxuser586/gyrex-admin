<plugin>

   <extension
         point="org.eclipse.rap.ui.entrypoint">
      <entrypoint
            class="org.eclipse.gyrex.admin.web.rap.internal.AdminRapApplication"
            parameter="admin"
            id="org.eclipse.gyrex.admin.web.rap.internal.AdminRapApplication">
      </entrypoint>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            name="Gyrex Admininistration"
            class="org.eclipse.gyrex.admin.web.rap.internal.Perspective"
            id="org.eclipse.gyrex.admin.web.rap.perspective">
      </perspective>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <view
            name="Gyrex Admin"
            class="org.eclipse.gyrex.admin.web.rap.internal.View"
            id="org.eclipse.gyrex.admin.web.rap.view">
      </view>
   </extension>
   <extension
         point="org.eclipse.rap.ui.branding">
      <branding
            defaultEntrypointId="org.eclipse.gyrex.admin.web.rap.internal.AdminRapApplication"
            id="org.eclipse.gyrex.admin.web.rap.branding.internal.AdminRapApplication"
            servletName="admin-rap"
            title="Gyrex Admin">
         <httpServiceSelector
               filter="(other.info=org.eclipse.gyrex.admin.http)">
         </httpServiceSelector>
      </branding>
   </extension>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="org.eclipse.gyrex.admin.web.rap.internal.GyrexAdminEditorFactory"
            default="false"
            icon="icons/gyrex.png"
            id="org.eclipse.gyrex.admin.web.rap.widgetEditor"
            name="Gyrex Admin">
      </editor>
   </extension>

</plugin>
