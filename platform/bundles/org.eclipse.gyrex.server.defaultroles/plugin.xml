<plugin>
   <extension
         point="org.eclipse.cloudfree.boot.serverroles">
      <role
            defaultStart="inDevelopmentMode"
            id="admin"
            name="CloudFree Admin UI (GWT based)">
         <requireBundle
               symbolicName="org.eclipse.equinox.http.registry">
         </requireBundle>
         <requireBundle
               symbolicName="org.eclipse.cloudfree.admin">
         </requireBundle>
         <requireBundle
               symbolicName="org.eclipse.cloudfree.admin.web.gwt.client">
         </requireBundle>
         <requireBundle
               symbolicName="org.eclipse.cloudfree.admin.web">
         </requireBundle>
      </role>
      <role
            defaultStart="inDevelopmentMode"
            id="http"
            name="CloudFree HTTP Server">
         <requireBundle
               symbolicName="org.eclipse.equinox.http.registry">
         </requireBundle>
         <requireBundle
               symbolicName="org.eclipse.cloudfree.http">
         </requireBundle>
      </role>
   </extension>

</plugin>
