<project default="publish" basedir=".">

	<available file="bin.gwt/org.eclipse.cloudfree.examples.bugsearch.gwt.internal.BugSearch" property="frontend.exists" />

	<target name="publish" depends="sync.frontend">
		<echo level="info" message="Done!" />
	</target>

	<target name="sync.frontend" if="frontend.exists">		<sync todir="frontend" includeemptydirs="true">			<fileset dir="bin.gwt/org.eclipse.cloudfree.examples.bugsearch.gwt.internal.BugSearch"/>		</sync>		<!--
		<echo level="info" message="Removing existing cache files" />
		<delete>
			<fileset dir="frontend">
				<include name="*.cache.*" />
			</fileset>
		</delete>
		<echo level="info" message="Synchronizing frontend files..." />
		<copy todir="frontend">
			<fileset dir="bin.gwt/org.eclipse.cloudfree.examples.bugsearch.gwt.internal.BugSearch"/>
		</copy>		-->
	</target>

</project>