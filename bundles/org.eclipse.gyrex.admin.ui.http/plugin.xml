<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.gyrex.admin.ui.configurationPages">
      <page
            class="org.eclipse.gyrex.admin.ui.http.internal.HttpApplicationPage"
            id="org.eclipse.gyrex.admin.ui.http.page"
            name="Applications"
            parentId="org.eclipse.gyrex.admin.ui.general">
      </page>
   </extension>
   <extension
         point="org.eclipse.core.runtime.adapters">
      <factory
            adaptableType="org.eclipse.gyrex.http.internal.application.manager.ApplicationRegistration"
            class="org.eclipse.gyrex.admin.ui.http.internal.HttpUiAdapterFactory">
         <adapter
               type="org.eclipse.ui.model.IWorkbenchAdapter">
         </adapter>
         <adapter
               type="org.eclipse.ui.views.properties.IPropertySource">
         </adapter>
      </factory>
      <factory
            adaptableType="org.eclipse.gyrex.http.internal.application.manager.ApplicationProviderRegistration"
            class="org.eclipse.gyrex.admin.ui.http.internal.HttpUiAdapterFactory">
         <adapter
               type="org.eclipse.ui.model.IWorkbenchAdapter">
         </adapter>
         <adapter
               type="org.eclipse.ui.views.properties.IPropertySource">
         </adapter>
      </factory>
   </extension>

</plugin>
